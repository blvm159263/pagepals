type Record {
    id: ID!
    recordingCount: Int
    startTime: String
    duration: Int
    externalId: String
    status: Status
    recordFiles: [RecordFile]
}

type RecordFile {
    id: ID!
    downloadUrl: String
    playUrl: String
    fileExtention: String
    fileType: String
    startAt: String
    endAt: String
    recordingType: String
    status: Status
}
